apiVersion: networking.istio.io/v1alpha3
kind: virtual-service
metadata:
  creationTimestamp: null
  name: c
  namespace: default
spec:
  hosts:
  - c
  http:
  - match:
    - headers:
        testwebsocket:
          exact: enabled
    route:
    - destination:
        name: c
        subset: version-v1
    websocketUpgrade: true
  - route:
    - destination:
        name: c
        subset: version-v1
      weight: 100
