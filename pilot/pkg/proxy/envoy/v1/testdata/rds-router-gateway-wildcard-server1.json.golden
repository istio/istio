       "operation": "gateway-wildcard-simple"
      }
     }
    ]
   },
   {
    "name": "some-gateway-wildcard|10080|world.example.com",
    "domains": [
     "world.example.com:10080",
     "world.example.com"
    ],
    "routes": [
     {
      "prefix": "/",
      "cluster": "out.world.default.svc.cluster.local|http",
      "timeout_ms": 0,
      "decorator": {
       "operation": "gateway-simple-2"
      }
     }
    ]
   }
  ]
 }