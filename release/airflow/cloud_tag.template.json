{
  "steps": [
    {
      "name": "gcr.io/istio-testing/istio-builder:0.5.10",
      "args": [ "gsutil", "cp", "-P", "gs://$_CB_GCS_RELEASE_TOOLS_PATH/*", "." ]
    },
    {
      "name": "gcr.io/istio-testing/istio-builder:0.5.10",
      "args": [ "./istio_checkout_code.sh" ]
    },
    {
      "volumes": [
	{
          "name": "buildoutput",
          "path": "/output"
	}
      ],
      "name": "gcr.io/istio-testing/istio-builder:0.5.10",
      "dir": "go/src/istio.io/istio",
      "args": [ "./release/gcb/publish_release.sh", "-w", "-x", "-e", "$_USER_EMAIL", "-n", "$_USER_NAME", "-g", "$_GCS_SOURCE", "-l", "$_GCS_SECRET", "-m", "-o", "$_GITHUB_ORG", "-v", "$_VERSION" ]
    }
  ],
  "timeout": "3600s",
}
