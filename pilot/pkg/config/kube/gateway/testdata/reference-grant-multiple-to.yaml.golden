apiVersion: networking.istio.io/v1
kind: Gateway
metadata:
  annotations:
    internal.istio.io/gateway-semantics: gateway
    internal.istio.io/gateway-service: istio-ingressgateway.bookinfo-ingress.svc.domain.suffix
    internal.istio.io/parents: Gateway/gateway/default.bookinfo-ingress
    internal.istio.io/service-account-name: ""
  name: gateway~istio-autogenerated-k8s-gateway~default
  namespace: bookinfo-ingress
spec:
  servers:
  - hosts:
    - '*/*.example.com'
    port:
      name: default
      number: 443
      protocol: HTTPS
    tls:
      credentialName: kubernetes-gateway://bookinfo/bookinfo-secret
      mode: SIMPLE
---
