{
  "steps": [
    {
      "name": "gcr.io/istio-testing/istio-builder:0.5.10",
      "args": [ "git", "clone", "https://github.com/$_CB_GITHUB_ORG/istio", "-b", "$_CB_BRANCH", "--depth", "1" ]
    },
    {
      "name": "gcr.io/istio-testing/istio-builder:0.5.10",
      "args": [ "gsutil", "cp", "-P", "gs://$_CB_GCS_RELEASE_TOOLS_PATH/*", "." ]
    },
    {
      "name": "gcr.io/istio-testing/istio-builder:0.5.10",
      "args": [ "istio/release/gcb/generate_manifest.sh" ]
    }
  ],
  "options": {
    "machineType": "N1_HIGHCPU_8"
  },
  "timeout": "3600s",
}
