apiVersion: networking.istio.io/v1
kind: Gateway
metadata:
  annotations:
    internal.istio.io/gateway-semantics: gateway
    internal.istio.io/gateway-service: with-tls-passthrough.istio-system.svc.domain.suffix
    internal.istio.io/parents: Gateway/with-tls-passthrough/tls-passthrough.istio-system
    internal.istio.io/service-account-name: with-tls-passthrough
  name: with-tls-passthrough~istio-autogenerated-k8s-gateway~tls-passthrough
  namespace: istio-system
spec:
  servers:
  - hosts:
    - istio-system/*
    port:
      name: default
      number: 6443
      protocol: TLS
    tls: {}
---
