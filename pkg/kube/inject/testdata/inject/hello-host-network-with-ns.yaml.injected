apiVersion: apps/v1
kind: Deployment
metadata:
  name: hello-host-network
  namespace: sample
spec:
  replicas: 7
  selector:
    matchLabels:
      app: hello-host-network
      tier: backend
      track: stable
  strategy: {}
  template:
    metadata:
      labels:
        app: hello-host-network
        tier: backend
        track: stable
    spec:
      containers:
      - image: fake.docker.io/google-samples/hello-go-gke:1.0
        name: hello-host-network
        ports:
        - containerPort: 80
          name: http
        resources: {}
      hostNetwork: true
status: {}
---
