{
    "concurrency": 4,
    "enableGC": true,
    "vendoredLinters": true,
    "deadline": "600s",
    "minConfidence": 0,
    "lineLength": 160,
    "enable": [
        "aligncheck",
        "deadcode",
        "errcheck",
        "gas",
        "goconst",
        "gofmt",
        "goimports",
        "golint",
        "ineffassign",
        "interfacer",
        "lll",
        "megacheck",
        "misspell",
        "structcheck",
        "unconvert",
        "varcheck",
        "vet",
        "vetshadow"
    ],
    "severity": {
        "aligncheck": "error",
        "deadcode": "error",
        "errcheck": "error",
        "gas": "error",
        "goconst": "error",
        "gofmt": "error",
        "goimports": "error",
        "golint": "error",
        "ineffassign": "error",
        "interfacer": "error",
        "lll": "error",
        "megacheck": "error",
        "misspell": "error",
        "structcheck": "error",
        "unconvert": "error",
        "varcheck": "error",
        "vet": "error",
        "vetshadow": "error"
    },
    "exclude": [
        "vendor",
        ".pb.go",
        "mixer/pkg/config/proto/combined.go",
        ".*.gen.go",
        "should have a package comment",
        ".*pkg/config/apiserver_test.go:.* method WriteHeaderAndJson should be WriteHeaderAndJSON",
        "genfiles"
    ]
}
