apiVersion: networking.istio.io/v1
kind: Gateway
metadata:
  annotations:
    internal.istio.io/gateway-semantics: gateway
    internal.istio.io/gateway-service: istio-ingressgateway.istio-system.svc.domain.suffix
    internal.istio.io/parents: Gateway/gateway/default.istio-system
    internal.istio.io/service-account-name: ""
  name: gateway~istio-autogenerated-k8s-gateway~default
  namespace: istio-system
spec:
  servers:
  - hosts:
    - '*/*.domain.example'
    port:
      name: default
      number: 80
      protocol: HTTP
---
apiVersion: networking.istio.io/v1
kind: VirtualService
metadata:
  annotations:
    internal.istio.io/parents: GRPCRoute/grpc.default,HTTPRoute/http.default
    internal.istio.io/route-semantics: gateway
  name: istio-system~gateway~istio-autogenerated-k8s-gateway~default~my.domain.example
  namespace: default
spec:
  gateways:
  - istio-system/gateway~istio-autogenerated-k8s-gateway~default
  hosts:
  - my.domain.example
  http:
  - match:
    - uri:
        prefix: /foo.FooService/
    name: default.grpc.0
    route:
    - destination:
        host: grpcbin.default.svc.domain.suffix
        port:
          number: 9090
  - match:
    - uri:
        prefix: /api
    name: default.http.0
    route:
    - destination:
        host: httpbin.default.svc.domain.suffix
        port:
          number: 80
---
